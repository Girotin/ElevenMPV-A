			<list_item id="list_device_05" title="msg_option_device_5" value="5" />
		</list>
		<list id="list_sort" title="msg_settings_sort" key="sort" default_value="0">
			<list_item id="list_sort_00" title="msg_option_sort_0" value="0" />
			<list_item id="list_sort_01" title="msg_option_sort_1" value="1" />
