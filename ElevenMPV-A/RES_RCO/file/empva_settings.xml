				<list_item id="list_audio_eq_04" title="msg_option_audio_eq_4" value="4" />
			</list>			
			<toggle_switch id="toggle_audio_alc" title="msg_settings_audio_alc" key="alc_mode" default_value="0"/>
			<toggle_switch id="toggle_audio_eqvol" title="msg_settings_audio_limit" key="eq_volume" default_value="0"/>
		</setting_list>
		<setting_list id="setting_list_power" title="msg_settings_power" style="edit">
